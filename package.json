    "babel-register": "^6.11.6",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "mocha": "^3.0.2",
    "should": "^10.0.0"
  },
  "dependencies": {
    "command-line-args": "^3.0.1"
  }
}
